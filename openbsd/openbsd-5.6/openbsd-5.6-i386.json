{
  "variables": {
    "iso_image": "cd56.iso",
    "path_to_iso": "OpenBSD/5.6/i386",
    "iso_checksum": "b34765e1c79c7e2fbf4adc5d8f9769ad6bf8c3f3e0b96928aa33e9811ac192d7",
    "box_version": "1.1"
  },
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "i<enter><wait>",
        "default<enter><wait>",
        "foo<enter><wait>",
        "em0<enter><wait>",
        "dhcp<enter><wait>",
        "none<enter><wait>",
        "done<enter><wait>",
        "vagrant<enter><wait>",
        "vagrant<enter><wait>",
        "yes<enter><wait>",
        "no<enter><wait>",
        "no<enter><wait>",
        "no<enter><wait>",
        "no<enter><wait>",
        "vagrant<enter><wait>",
        "vagrant<enter><wait>",
        "vagrant<enter><wait>",
        "vagrant<enter><wait>",
        "yes<enter><wait>",
        "UTC<enter><wait>",
        "wd0<enter><wait>",
        "yes<enter><wait>",
        "whole<enter><wait>",
        "a<enter><wait>",
        "http<enter><wait>",
        "none<enter><wait>",
        "ftp3.usa.openbsd.org<enter><wait>",
        "<enter><wait>",
        "-x*<enter><wait>",
        "-game*<enter><wait>",
        "<enter><wait>",
        "<wait10><wait10><wait10><wait10><wait10><wait10>",
        "<wait10><wait10><wait10>",
        "done<enter><wait>",
        "ftp -o /tmp/install.sh http://{{ .HTTPIP }}:{{ .HTTPPort }}/install.sh<enter><wait>",
        "sh /tmp/install.sh<enter><wait>",
        "reboot<enter>"
      ],
      "boot_wait": "30s",
      "disk_size": 10240,
      "guest_os_type": "OpenBSD",
      "http_directory": "../http",
      "iso_checksum": "{{user `iso_checksum`}}",
      "iso_checksum_type": "sha256",
      "iso_urls": [
        "iso/{{user `iso_image`}}",
        "http://www.ftp.ne.jp/{{user `path_to_iso`}}/{{user `iso_image`}}",
        "http://ftp3.usa.openbsd.org/pub/{{user `path_to_iso`}}/{{user `iso_image`}}"
      ],
      "ssh_username": "root",
      "ssh_password": "vagrant",
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "shutdown -h -p now",
      "guest_additions_mode": "disable",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "1024"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--rtcuseutc",
          "on"
        ]
      ]
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "scripts": [
        "../provisioners/rsync.sh",
        "../provisioners/sharity-light.sh",
        "../provisioners/vagrant.sh",
        "../provisioners/cleanup.sh"
      ],
      "environment_vars": [
        "PKG_PATH=http://ftp3.usa.openbsd.org/pub/OpenBSD/5.6/packages/i386/"
      ]
    }
  ],
  "post-processors": [
    {
      "output": "./OpenBSD-5.6-i386-v{{user `box_version`}}.box",
      "type": "vagrant",
      "compression_level": 9,
      "override": {
        "virtualbox": {
          "vagrantfile_template": "../vagrantfiles/Vagrantfile.OpenBSD"
        }
      }
    }
  ]
}

{
  "variables": {
    "iso_image": "FreeBSD-10.3-RELEASE-i386-disc1.iso",
    "path_to_iso": "FreeBSD/releases/ISO-IMAGES/10.3",
    "iso_checksum": "8329ee2ca4779892edbb001c303670d74642e4353e97d02f4521895023cacd5d",
    "iso_checksum_type": "sha256",
    "os_ver": "10.3",
    "box_ver": "1"
  },
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "<enter>",
        "<wait10><wait10>",
        "s",
        "<wait5>",
        "dhclient em0<enter><wait5>",
        "fetch -o /tmp/install.sh http://{{ .HTTPIP }}:{{ .HTTPPort }}/install.sh<enter>",
        "<wait>",
        "DISTRIBUTIONS='base.txz kernel.txz' ",
        "BSDINSTALL_DISTSITE=ftp://ftp.freebsd.org/pub/FreeBSD/releases/i386/i386/10.3-RELEASE ",
        "ABI=FreeBSD:10:i386 bsdinstall script /tmp/install.sh<enter>",
        "<wait10><wait10><wait10>",
        "shutdown -r now<enter>"
      ],
      "boot_wait": "10s",
      "disk_size": 51200,
      "guest_os_type": "FreeBSD",
      "http_directory": ".",
      "iso_checksum": "{{user `iso_checksum` }}",
      "iso_checksum_type": "{{user `iso_checksum_type`}}",
      "iso_urls": [
        "./iso/{{user `iso_image`}}",
        "http://ftp10.freebsd.org/pub/{{user `path_to_iso`}}/{{user `iso_image`}}",
        "http://ftp.freebsd.org/pub/{{user `path_to_iso`}}/{{user `iso_image`}}",
        "http://ftp4.us.freebsd.org/pub/{{user `path_to_iso`}}/{{user `iso_image`}}",
        "http://ftp13.us.freebsd.org/pub/{{user `path_to_iso`}}/{{user `iso_image`}}",
        "http://ftp6.tw.freebsd.org/pub/{{user `path_to_iso`}}/{{user `iso_image`}}",
        "http://ftp11.tw.freebsd.org/{{user `path_to_iso`}}/{{user `iso_image`}}"
      ],
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_key_path": "../key/vagrant_private_key",
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "echo 'vagrant' | sudo shutdown -p now",
      "guest_additions_mode": "disable",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "1024"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--rtcuseutc",
          "on"
        ]
      ]
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "scripts": [
        "../provisioners/vagrant-10.3.sh",
        "../provisioners/cleanup.sh"
      ],
      "override": {
        "virtualbox-iso": {
          "execute_command": "echo 'vagrant' | sudo sh '{{.Path}}'"
        }
      }
    }
  ],
  "post-processors": [
    {
      "output": "./FreeBSD-{{user `os_ver`}}-RELEASE-i386.box",
      "type": "vagrant",
      "override": {
        "virtualbox": {
          "vagrantfile_template": "../vagrantfiles/Vagrantfile.FreeBSD-10.3"
        }
      }
    }
  ]
}

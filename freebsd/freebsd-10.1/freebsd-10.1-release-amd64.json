{
  "variables": {
    "path_to_iso": "ISO-IMAGES-amd64/10.1/FreeBSD-10.1-RELEASE-amd64-disc1.iso",
    "iso_checksum": "d0d4553080b88653569e1871d57fc819",
    "box_ver": "1"
  },
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "<enter>",
        "<wait10><wait5>",
        "s",
        "<wait5>",
        "dhclient em0<enter><wait5>",
        "fetch -o /tmp/install.sh http://{{ .HTTPIP }}:{{ .HTTPPort }}/install.sh<enter>",
        "<wait>",
        "bsdinstall script /tmp/install.sh<enter>",
        "<wait10><wait10><wait10>",
        "shutdown -r now<enter>"
      ],
      "boot_wait": "10s",
      "disk_size": 10240,
      "guest_os_type": "FreeBSD_64",
      "http_directory": ".",
      "iso_checksum": "{{user `iso_checksum` }}",
      "iso_checksum_type": "md5",
      "iso_urls": [
        "http://ftp10.freebsd.org/pub/FreeBSD/{{user `path_to_iso`}}",
        "http://ftp.freebsd.org/pub/FreeBSD/{{user `path_to_iso`}}",
        "http://ftp4.us.freebsd.org/pub/FreeBSD/{{user `path_to_iso`}}",
        "http://ftp13.us.freebsd.org/pub/FreeBSD/{{user `path_to_iso`}}",
        "http://ftp6.tw.freebsd.org/pub/FreeBSD/{{user `path_to_iso`}}",
        "http://ftp11.tw.freebsd.org/FreeBSD/{{user `path_to_iso`}}"
      ],
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_key_path": "../key/vagrant_private_key",
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "echo 'vagrant' | sudo shutdown -p now",
      "guest_additions_mode": "disable",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "1024"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--rtcuseutc",
          "on"
        ]
      ]
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "scripts": [
        "../provisioners/vagrant.sh",
        "../provisioners/virtualbox_ose_additions.sh",
        "../provisioners/cleanup.sh"
      ],
      "override": {
        "virtualbox-iso": {
          "execute_command": "echo 'vagrant' | sudo sh '{{.Path}}'"
        }
      }
    }
  ],
  "post-processors": [
    {
      "output": "./FreeBSD-10.1-RELEASE-amd64-v{{user `box_ver`}}.box",
      "type": "vagrant",
      "override": {
        "virtualbox": {
          "vagrantfile_template": "../vagrantfile/Vagrantfile.FreeBSD"
        }
      }
    }
  ]
}

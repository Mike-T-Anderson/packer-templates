  "provisioners": [
    {
      "type": "shell",
      "inline": [
        "echo \"PKG_PATH=http://ftp.NetBSD.org/pub/pkgsrc/packages/NetBSD/`uname -m`/7.0_2016Q3/All\" > /etc/pkg_install.conf"
      ],
      "inline_shebang": "/bin/sh -ex"
    },
    {
      "type": "shell",
      "only": ["vmware-iso", "esxi-iso"],
      "script": "../provisioners/vmware-xorg.sh",
      "execute_command": "chmod +x {{ .Path }}; {{ .Vars }} PATH=$PATH:/usr/sbin {{ .Path }}",
      "environment_vars": [
        "OPEN_VM_TOOLS=open-vm-tools-2008.08.08.109361nb45",
        "XF86_INPUT_VMMOUSE=xf86-input-vmmouse-13.1.0nb1",
        "XF86_VIDEO_VMWARE=xf86-video-vmware-13.1.0nb4"
      ]
    },
    {
      "type": "shell",
      "scripts": [
        "../provisioners/vagrant-7.0.sh",
        "../provisioners/dwm.sh"
      ],
      "execute_command": "chmod +x {{ .Path }}; {{ .Vars }} PATH=$PATH:/usr/sbin {{ .Path }}",
      "environment_vars": [
        "VAGRANT_USER={{user `user_name`}}",
        "SUDO=sudo-1.8.17p1",
        "RSYNC=rsync-3.1.2",
        "DWM=dwm-6.1",
        "DMENU=dmenu-4.6",
        "XRANDR=xrandr-1.5.0",
        "X11VNC=x11vnc-0.9.13nb27"
      ]
    },
    {
      "type": "shell",
      "only": ["virtualbox-iso", "vmware-iso"],
      "script": "../provisioners/disk_cleanup.sh"
    },
    {
      "type": "shell",
      "script": "../provisioners/cleanup-7.0.sh"
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "only": ["virtualbox-iso"],
      "compression_level": 9,
      "output": "./{{user `vm_name`}}-{{user `variant`}}-v{{user `box_ver`}}-{{.Provider}}.box",
      "vagrantfile_template": "../vagrantfiles/Vagrantfile.NetBSD-sh"
    },
    {
      "type": "vagrant",
      "only": ["vmware-iso"],
      "compression_level": 9,
      "output": "./{{user `vm_name`}}-{{user `variant`}}-v{{user `box_ver`}}-{{.Provider}}.box",
      "vagrantfile_template": "../vagrantfiles/Vagrantfile.NetBSD-sh",
      "keep_input_artifact": true

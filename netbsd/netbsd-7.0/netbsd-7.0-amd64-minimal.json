{
  "variables": {
    "ssh_username": "root",
    "iso_file_name": "NetBSD-7.0-amd64.iso",
    "iso_checksum": "ec63aa3bfe6ddee2ae9cd801bbc2533524b29bca41266415ebd567151d19b0839f75cd75e1dd674632761a7658599684c4842e6458cd6e9171d5959d47f15ffb",
    "iso_checksum_type": "sha512",
    "os_ver": "7.0",
    "box_ver": "1.1"
  },
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_wait": "10s",
      "disk_size": 307200,
      "guest_os_type": "NetBSD_64",
      "iso_urls": [
        "{{user `iso_file_name`}}",
        "http://ftp.netbsd.org/pub/NetBSD/NetBSD-7.0/iso/{{user `iso_file_name`}}",
        "http://mirror.planetunix.net/pub/NetBSD/NetBSD-7.0/iso/{{user `iso_file_name`}}",
        "http://ftp7.de.NetBSD.org/pub/ftp.netbsd.org/pub/NetBSD/NetBSD-7.0/iso/{{user `iso_file_name`}}",
        "https://ftp.ipv6.uni-leipzig.de/pub/ftp.netbsd.org/pub/NetBSD/NetBSD-7.0/iso/{{user `iso_file_name`}}",
        "http://ftp.fr.NetBSD.org/pub/NetBSD/NetBSD-7.0/iso/{{user `iso_file_name`}}",
        "http://ftp.availo.se/pub/NetBSD/NetBSD-7.0/iso/{{user `iso_file_name`}}",
        "http://ftp.fi.NetBSD.org/pub/NetBSD/NetBSD-7.0/iso/{{user `iso_file_name`}}"
      ],
      "iso_checksum": "{{user `iso_checksum`}}",
      "iso_checksum_type": "{{user `iso_checksum_type`}}",
      "ssh_username": "{{user `ssh_username`}}",
      "ssh_password": "vagrant",
      "ssh_key_path": "../key/vagrant_private_key",
      "ssh_port": 22,
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "echo 'shutdown -p now'|su -",
      "guest_additions_mode": "disable",
      "virtualbox_version_file": ".vbox_version",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "512"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--rtcuseutc",
          "on"
        ]
      ],
      "http_directory": ".",
      "boot_command": [
        "1<wait10><wait10><wait5>",
        "a<enter><wait>",
        "a<enter><wait>",
        "a<enter><wait>",
        "b<enter><wait>",
        "a<enter><wait>",
        "a<enter><wait>",
        "b<enter><wait>",
        "a<enter><wait>",
        "b<enter><wait>",
        "x<enter><wait>",
        "<enter><wait>",
        "b<enter><wait10>",
        "<wait10><wait10><wait10><wait10><wait10><wait10>",
        "a<enter><wait>",
        "x<enter><wait>",
        "d<enter><wait>",
        "g<enter><wait>",
        "j<enter><wait>",
        "x<enter><wait>",
        "a<enter><wait10><wait10><wait10>",
        "<enter><wait>",
        "d<enter><wait>",
        "a<enter><wait>",
        "vagrant<enter><wait>",
        "vagrant<enter><wait>",
        "vagrant<enter><wait>",
        "g<enter><wait>",
        "h<enter><wait>",
        "o<enter><wait>",
        "vagrant<enter><wait>",
        "a<enter><wait>",
        "b<enter><wait>",
        "vagrant<enter><wait>",
        "vagrant<enter><wait>",
        "vagrant<enter><wait>",
        "x<enter><wait>",
        "<enter><wait10>",
        "x<enter><wait>",
        "dhclient<enter><wait10>",
        "ftp -o /tmp/install.sh http://{{ .HTTPIP }}:{{ .HTTPPort }}/install.sh<enter><wait>",
        "HTTPSERVER={{ .HTTPIP }}:{{ .HTTPPort }} sh /tmp/install.sh<enter><wait>"
      ]
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "scripts": [
        "../provisioners/sudo.sh",
        "../provisioners/vagrant-7.0.sh",
        "../provisioners/rsync-7.0.sh",
        "../provisioners/cleanup-7.0.sh"
      ],
      "override": {
        "virtualbox-iso": {
          "execute_command": "echo 'PKG_PATH=ftp://ftp.NetBSD.org/pub/pkgsrc/packages/`uname -s`/`uname -m`/7.0_2015Q3/All/; export PKG_PATH' | cat - '{{.Path}}' | su -"
        }
      }
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "output": "./NetBSD-{{user `os_ver`}}-amd64-minimal-v{{user `box_ver`}}.box",
      "override": {
        "virtualbox": {
          "vagrantfile_template": "../vagrantfile/Vagrantfile.NetBSD"
        }
      }
    }
  ]
}

  "provisioners": [
    {
      "type": "shell",
      "scripts": [
        "../provisioners/sudo.sh",
        "../provisioners/vagrant-7.0.sh",
        "../provisioners/rsync-7.0.sh",
        "../provisioners/cleanup-7.0.sh"
      ],
      "override": {
        "virtualbox-iso": {
          "execute_command": "echo 'PKG_PATH=ftp://ftp.NetBSD.org/pub/pkgsrc/packages/`uname -s`/`uname -m`/7.0_2015Q3/All/; export PKG_PATH' | cat - '{{.Path}}' | su -"
        },
        "vmware-iso": {
          "execute_command": "echo 'PKG_PATH=ftp://ftp.NetBSD.org/pub/pkgsrc/packages/`uname -s`/`uname -m`/7.0_2015Q3/All/; export PKG_PATH' | cat - '{{.Path}}' | su -"
        }
      }
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "compression_level": 9,
      "output": "./NetBSD-{{user `os_ver`}}-i386-minimal-v{{user `box_ver`}}-{{.Provider}}.box",
      "vagrantfile_template": "../vagrantfiles/Vagrantfile.NetBSD"
